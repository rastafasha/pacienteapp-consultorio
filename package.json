{
  "name": "pacienteapp-consultorio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --port 4203",
    "start:prod": "ng build && lite-server -p 8080 --baseDir dist/pacienteapp-consultorio",
    "build": "ng build",
    "build:paciente": "ng build --configuration paciente",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:dev": "ng test --browsers=ChromeHeadless --watch=true --code-coverage",
    "lint": "ng eslint . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "latest",
    "@fullcalendar/angular": "^6.1.4",
    "@fullcalendar/daygrid": "^6.1.4",
    "@fullcalendar/interaction": "^6.1.4",
    "@fullcalendar/list": "^6.1.4",
    "lite-server": "^2.6.1",
    "ng-apexcharts": "^1.7.4",
    "ng-circle-progress": "^1.7.1",
    "ngx-material-timepicker": "^13.1.1",
    "rxjs": "~7.5.0",
    "sweetalert2": "^11.10.2",
    "tslib": "^2.3.0",
    "typescript": "5.4.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^17.0.0",
    "@capacitor/cli": "latest",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "^5.4.0"
  }
}
